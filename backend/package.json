{
  "name": "myroom-backend",
  "version": "1.0.0",
  "description": "MyRoom Backend API - 3D Resource Management Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register -r module-alias/register dist/index.js",
    "start:prod": "NODE_ENV=production node -r tsconfig-paths/register -r module-alias/register dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:init-items": "tsx scripts/import-items.ts --dir ../resources/items",
    "db:init-avatars": "tsx scripts/import-avatars.ts --dir ../resources/avatars",
    "db:init-rooms": "tsx scripts/import-rooms.ts --dir ../resources/rooms",
    "db:init-animations": "tsx scripts/import-animations.ts --dir ../resources/animations",
    "admin:create": "tsx src/scripts/create-admin.js",
    "deploy:setup": "npm run db:generate && npm run db:deploy && npm run db:seed",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "3d",
    "babylon.js",
    "api",
    "typescript",
    "express",
    "postgresql",
    "aws",
    "s3"
  ],
  "author": "MyRoom Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.850.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-storage": "^3.842.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@prisma/client": "^6.12.0",
    "@types/ioredis": "^4.28.10",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cuid": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.12",
    "sharp": "^0.33.1",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime-types": "^3.0.1",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.6",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^6.12.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "tsx src/scripts/seed.ts"
  },
  "_moduleAliases": {
    "@": "dist",
    "@/config": "dist/config",
    "@/controllers": "dist/controllers",
    "@/middleware": "dist/middleware",
    "@/models": "dist/models",
    "@/routes": "dist/routes",
    "@/services": "dist/services",
    "@/utils": "dist/utils",
    "@/types": "dist/types",
    "@/schemas": "dist/schemas",
    "@/validators": "dist/validators"
  }
}
